  3.714859049697453*^9}},ExpressionUUID->"81e965f6-cdb6-401a-976e-\
6089a49a9a4e"],

Cell["\<\
The function mid uses RegionCentroid to compute the centroid of the Triangle \
represented by T, note that this function will work for more general regions \
as well. \
\>", "Text",
 CellChangeTimes->{{3.7148589157960997`*^9, 
  3.7148589742591057`*^9}},ExpressionUUID->"3f3c895e-f577-4de4-972f-\
d6be151b3006"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rot", "[", 
   RowBox[{"T_", ",", "t_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"r", ",", "s", ",", "Q"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"r", "=", 
      RowBox[{
       RowBox[{"mid", "[", "T", "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"s", "=", 
      RowBox[{
       RowBox[{"mid", "[", "T", "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Q", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Cos", "[", "t", "]"}], ",", 
          RowBox[{"-", 
           RowBox[{"Sin", "[", "t", "]"}]}], ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Sin", "[", "t", "]"}], ",", 
          RowBox[{"Cos", "[", "t", "]"}], ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"trans", "[", 
      RowBox[{
       RowBox[{"Q", ".", 
        RowBox[{"trans", "[", 
         RowBox[{"T", ",", 
          RowBox[{"-", "r"}], ",", 
          RowBox[{"-", "s"}]}], "]"}]}], ",", "r", ",", "s"}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.714857536889242*^9, 3.7148575802847977`*^9}, {
  3.714859102009664*^9, 3.714859107670944*^9}, {3.714859235747114*^9, 
