Cell[CellGroupData[{

Cell["Put it all together in a GIF", "Subsection",
 CellChangeTimes->{{3.71494316539252*^9, 
  3.71494317061467*^9}},ExpressionUUID->"75d66fa8-e6cd-498b-b411-\
e5e6a8e7b56e"],

Cell["\<\
At this point, we are ready to put all of our transformations together in one \
GIF. To this end, we will move our spaceship around using translations, \
rotations, and reflections. We compose these transformations one at a time, \
and in the process form a list of graphics. These lists are stored in the \
variable anim, and then exported to a gif file using the Export command. \
\>", "Text",
 CellChangeTimes->{{3.714947015978896*^9, 3.714947076862054*^9}, {
  3.714947273966146*^9, 3.714947278758286*^9}, {3.714947501448975*^9, 
  3.714947504735573*^9}, {3.71494759606463*^9, 3.7149476367195587`*^9}, {
  3.714947670000876*^9, 
  3.714947677302602*^9}},ExpressionUUID->"9c6a4bbc-098b-43e6-9913-\
e58454e0ffef"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"anim", "=", 
  RowBox[{"List", "[", 
   RowBox[{
    RowBox[{"Graphics", "[", 
     RowBox[{
      RowBox[{"help", "[", "T", "]"}], ",", 
      RowBox[{"Axes", "\[Rule]", "True"}], ",", 
      RowBox[{"PlotRange", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "5"}], ",", "5"}], "}"}]}], "}"}]}]}], "]"}], ",", 
    RowBox[{"Graphics", "[", 
     RowBox[{
      RowBox[{"help", "[", 
       RowBox[{"trans", "[", 
        RowBox[{"T", ",", "0", ",", "1"}], "]"}], "]"}], ",", 
      RowBox[{"Axes", "\[Rule]", "True"}], ",", 
      RowBox[{"PlotRange", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "5"}], ",", "5"}], "}"}]}], "}"}]}]}], "]"}], ",", 
    RowBox[{"Graphics", "[", 
     RowBox[{
      RowBox[{"help", "[", 
       RowBox[{"rot", "[", 
        RowBox[{
         RowBox[{"trans", "[", 
          RowBox[{"T", ",", "0", ",", "1"}], "]"}], ",", 
         RowBox[{"Pi", "/", "4"}]}], "]"}], "]"}], ",", 
      RowBox[{"Axes", "\[Rule]", "True"}], ",", 
      RowBox[{"PlotRange", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
