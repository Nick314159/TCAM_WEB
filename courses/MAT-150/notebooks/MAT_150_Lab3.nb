2879e6dfe2f3"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"mid", "[", "T_", "]"}], ":=", 
   RowBox[{"RegionCentroid", "[", 
    RowBox[{"help", "[", "T", "]"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7148587769440327`*^9, 3.71485878841667*^9}, {
  3.7148588185724277`*^9, 3.7148588412746487`*^9}, {3.7148589060072927`*^9, 
