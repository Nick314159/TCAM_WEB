   RowBox[{"trans", "[", 
    RowBox[{"T_", ",", "r_", ",", "s_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"1", ",", "0", ",", "r"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1", ",", "s"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "}"}], ".", "T"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.714856307321425*^9, 3.7148563271115723`*^9}, {
  3.7148563672159777`*^9, 3.714856400494759*^9}, {3.714944381939899*^9, 
