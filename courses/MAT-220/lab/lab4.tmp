\begin{pythontexcustomcode}{py}
def FindPrimes(start,end):
    x=[]
    if start==1:
        start=start+1
    for i in range(start,end+1):
        check=True
        for j in range(2,i):
            if i%j==0:
                check=False
                break
        if check==True:
            x=x+[i]
    return x
\end{pythontexcustomcode}
